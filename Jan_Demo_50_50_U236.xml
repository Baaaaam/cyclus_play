<simulation>
  <control>
    <duration>200</duration>
    <startmonth>1</startmonth>
    <startyear>1960</startyear>
    <simhandle>NuKe</simhandle>
  </control>

  <archetypes>
    <spec><lib>cycamore</lib><name>Source</name></spec>
    <spec><lib>cycamore</lib><name>Reactor</name></spec>
    <spec><lib>cycamore</lib><name>Separations</name></spec>
    <spec><lib>cycamore</lib><name>Sink</name></spec>
    <spec><lib>agents</lib><name>NullInst</name></spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>cycvt</lib><name>SEnrichment</name></spec>
  </archetypes>


  <commodity>
    <name>Original_Fuel</name>
    <solution_priority>1</solution_priority>
  </commodity>

  <commodity>
    <name>Spent_Original_Fuel</name>
    <solution_priority>1</solution_priority>
  </commodity>

  <commodity>
    <name>Recycled_Fuel</name>
    <solution_priority>1</solution_priority>
  </commodity>

  <commodity>
    <name>Spent_Recycled_Fuel</name>
    <solution_priority>1</solution_priority>
  </commodity>

  <commodity>
    <name>Separated_Pu</name>
    <solution_priority>1</solution_priority>
  </commodity>

  <commodity>
    <name>Separated_U_for_Reactor_Re-use</name>
    <solution_priority>1</solution_priority>
  </commodity>

  <commodity>
    <name>Separated_U_for_HEU_enr</name>
    <solution_priority>1</solution_priority>
  </commodity>

  <commodity>
    <name>Separated_Waste</name>
    <solution_priority>1</solution_priority>
  </commodity>

  <commodity>
    <name>Separated_U_Store</name>
    <solution_priority>1</solution_priority>
  </commodity>
  
    <commodity>
    <name>Enrich_Tails_Reactor_Re-use</name>
    <solution_priority>1</solution_priority>
  </commodity>
  
      <commodity>
    <name>HEU</name>
    <solution_priority>1</solution_priority>
  </commodity>
  
     <commodity>
    <name>Enrich_Tails_HEU_prod</name>
    <solution_priority>1</solution_priority>
  </commodity>


  <recipe>
    <name>Original_Fuel_Recipe</name>
    <basis>mass</basis>
    <nuclide><id>U-235</id><comp>0.007</comp></nuclide>
    <nuclide><id>U-238</id><comp>0.993</comp></nuclide>
  </recipe>

  <recipe>
    <name>Spent_Original_Fuel_Recipe</name>                                     <!-- Here without decay. Generally highest activities (in this order) for cs-137, ba-137m, sr-90, y-90, am-241 -->
    <basis>mass</basis>
    <nuclide><id>U-235</id><comp>0.00658</comp></nuclide>
    <nuclide><id>U-236</id><comp>6.831E-05</comp></nuclide>
    <nuclide><id>U-238</id><comp>0.9929</comp></nuclide>
    <nuclide><id>Pu-239</id><comp>0.00036</comp></nuclide>
    <nuclide><id>Pu-240</id><comp>1.4576E-05</comp></nuclide>
    <nuclide><id>Pu-241</id><comp>6.1452E-07</comp></nuclide>
    <nuclide><id>Np-237</id><comp>1.2037E-06</comp></nuclide>
    <nuclide><id>Np-239</id><comp>5.1312E-06</comp></nuclide>
    <nuclide><id>Cs-137</id><comp>1.4637E-05</comp></nuclide>
    <nuclide><id>Sr-90</id><comp>8.3414E-06</comp></nuclide>
  </recipe>

  <recipe>
    <name>Recycled_Fuel_Recipe</name>                                     <!-- Should request 7% enriched uranium, and get back 7% U-235, some U-236 and U-238 -->
    <basis>mass</basis>
    <nuclide><id>U-235</id><comp>0.007</comp></nuclide>
    <nuclide><id>U-238</id><comp>0.993</comp></nuclide>
    <!-- <nuclide><id>U-235</id><comp>0.007</comp></nuclide>
         <nuclide><id>U-238</id><comp>0.99293</comp></nuclide>
         <nuclide><id>U-236</id><comp>6.8344E-05</comp></nuclide> -->
  </recipe>

  <recipe>
    <name>Spent_Recycled_Fuel_Recipe</name>                                     <!-- Where does this come from? Reliable? -->
    <basis>mass</basis>
    <nuclide><id>U-235</id><comp>0.00658</comp></nuclide>
    <nuclide><id>U-236</id><comp>0.000136</comp></nuclide>
    <nuclide><id>U-238</id><comp>0.9929</comp></nuclide>
    <nuclide><id>Pu-239</id><comp>0.00036</comp></nuclide>
    <nuclide><id>Pu-240</id><comp>1.4575E-05</comp></nuclide>
    <nuclide><id>Pu-241</id><comp>6.1445E-07</comp></nuclide>
    <nuclide><id>Np-237</id><comp>1.43255E-06</comp></nuclide>
    <nuclide><id>Np-239</id><comp>5.1309E-06</comp></nuclide>
    <nuclide><id>Cs-137</id><comp>1.4637E-05</comp></nuclide>
    <nuclide><id>Sr-90</id><comp>8.3415E-06</comp></nuclide>
  </recipe>
  
  <recipe>
    <name>HEU_Recipe</name>                          <!-- Asks for 94% U-235 enrichment, will also get back U-236. -->
    <basis>mass</basis>
    <nuclide><id>U-235</id><comp>0.94</comp></nuclide>
    <nuclide><id>U-238</id><comp>0.06</comp></nuclide>
  </recipe>


  <facility>
    <name>U_Mine</name>
    <config>
      <Source>
        <outcommod>Original_Fuel</outcommod>
      </Source>
    </config>
  </facility>

  <facility>
    <name>Reactor</name>                                 <!-- Two reactors are simulated. -->
    <config>
      <Reactor>
        <fuel_incommods>
          <val>Original_Fuel</val>
          <val>Recycled_Fuel</val>
        </fuel_incommods>
        <fuel_inrecipes>
          <val>Original_Fuel_Recipe</val>
          <val>Recycled_Fuel_Recipe</val>
        </fuel_inrecipes>
        <fuel_prefs>                                 <!-- If preferences don't differ, no re-enriched fuel will be ordered. -->
          <val>1.0</val>
          <val>1.5</val>
        </fuel_prefs>
        <fuel_outcommods>
          <val>Spent_Original_Fuel</val>
          <val>Spent_Recycled_Fuel</val>
        </fuel_outcommods>
        <fuel_outrecipes>
          <val>Spent_Original_Fuel_Recipe</val>
          <val>Spent_Recycled_Fuel_Recipe</val>
        </fuel_outrecipes>
        <assem_size>70</assem_size>
        <n_assem_batch>848</n_assem_batch>
        <n_assem_core>1696</n_assem_core>
        <cycle_time>4</cycle_time>                       <!-- To reach correct burnup of 400 MWd/t at specific power of 1.53 MW/t, 261d are require, 2 batches -->
        <refuel_time>1</refuel_time>
        <n_assem_fresh>848</n_assem_fresh>
      </Reactor>
    </config>
  </facility>

  <facility>
    <name>Reprocessing_for_Re-use</name>
    <config>
      <Separations>
        <feed_commods><val>Spent_Original_Fuel</val></feed_commods>
        <feedbuf_size>1000000</feedbuf_size>
        <throughput>1000000</throughput>
        <leftover_commod>Separated_Waste</leftover_commod>
        <streams>
          <item>
            <commod>Separated_Pu</commod>
            <info>
              <buf_size>1000000</buf_size>
              <efficiencies>
                <item>
                  <comp>94000</comp>
                  <eff>0.99</eff>
                </item>
              </efficiencies>
            </info>
          </item>
          <item>
            <commod>Separated_U_for_Reactor_Re-use</commod>
            <info>
              <buf_size>1000000</buf_size>
              <efficiencies>
                <item>
                  <comp>92000</comp> 
                  <eff>0.495</eff>
                  </item>
                </efficiencies>
            </info>
          </item>
          <item>
            <commod>Separated_U_for_HEU_enr</commod>
            <info>
              <buf_size>1000000</buf_size>
              <efficiencies>
                <item>
                  <comp>92000</comp>
                   <eff>0.495</eff>
                  </item>
                </efficiencies>
            </info>
          </item>
        </streams>
      </Separations>
    </config>
  </facility>

  <facility>
    <name>Reprocessing_for_U_Storage</name>
    <config>
      <Separations>
        <feed_commods><val>Spent_Recycled_Fuel</val></feed_commods>
        <feedbuf_size>1000000</feedbuf_size>
        <throughput>1000000</throughput>
        <leftover_commod>Separated_Waste</leftover_commod>
        <streams>
          <item>
            <commod>Separated_Pu</commod>
            <info>
              <buf_size>1000000</buf_size>
              <efficiencies>
                <item>
                  <comp>94000</comp>
                  <eff>0.99</eff>
                </item>
              </efficiencies>
            </info>
          </item>
          <item>
            <commod>Separated_U_Store</commod>
            <info>
              <buf_size>1000000</buf_size>
              <efficiencies>
                <item>
                  <comp>92000</comp>
                  <eff>0.99</eff>
                </item>
              </efficiencies>
            </info>
          </item>
        </streams>
      </Separations>
    </config>
  </facility>

  <facility>
    <name>HLW_Storage</name>
    <config>
      <Sink>
        <in_commods><val>Separated_Waste</val></in_commods>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>Separated_Pu_Storage</name>
    <config>
      <Sink>
        <in_commods><val>Separated_Pu</val></in_commods>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>Separated_U_Storage</name>
    <config>
      <Sink>
        <in_commods><val>Separated_U_Store</val></in_commods>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>EnrichPlant_for_Reactor_Re-use</name>
    <config>
      <SEnrichment>
        <feed_commod>Separated_U_for_Reactor_Re-use</feed_commod>
        <feed_recipe>Spent_Original_Fuel_Recipe</feed_recipe>   <!-- Not sure how this place a role (it should not) -->
        <product_commod>Recycled_Fuel</product_commod>
        <tails_commod>Enrich_Tails_Reactor_Re-use</tails_commod>
        <max_feed_inventory>1000000</max_feed_inventory>
        <tails_assay>0.003</tails_assay>
        <enrich_efficiencies>
          <item>
            <comp>U-236</comp>
            <eff>0.98323</eff>         <!-- From www.wise-uranium.org/nfcjol.html, should get 0.007145% U-236 in product and 0.004161% in tails -->
          </item>
        </enrich_efficiencies>
      </SEnrichment>
    </config>
  </facility>

  <facility>
    <name>EnrichPlant_for_HEU_Prod</name>
    <config>
      <SEnrichment>
        <feed_commod>Separated_U_for_HEU_enr</feed_commod>
        <feed_recipe>Spent_Original_Fuel_Recipe</feed_recipe>   <!-- Not sure how this place a role (it should not) -->
        <product_commod>HEU</product_commod>
        <tails_commod>Enrich_Tails_HEU_prod</tails_commod>
        <max_feed_inventory>1000000</max_feed_inventory>
        <tails_assay>0.003</tails_assay>
        <enrich_efficiencies>
          <item>
            <comp>U-236</comp>
            <eff>0.44903</eff>         <!-- From www.wise-uranium.org/nfcjol.html, should get 0.438177% U-236 in product and 0.005178% in tails -->
          </item>
        </enrich_efficiencies>
      </SEnrichment>
    </config>
  </facility>
  
  <facility>
    <name>HEU_Storage</name>
    <config>
      <Sink>
        <in_commods><val>HEU</val></in_commods>
        <recipe_name>HEU_Recipe</recipe_name>
      </Sink>
    </config>
  </facility>


  <region>
    <name>Nu</name>
    <config><NullRegion/></config>
    <institution>
      <name>NuCoop</name>
      <config><NullInst/></config>
      <initialfacilitylist>
        <entry>
          <prototype>U_Mine</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>Reactor</prototype>
          <number>2</number>
        </entry>
        <entry>
          <prototype>Reprocessing_for_Re-use</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>Reprocessing_for_U_Storage</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>HLW_Storage</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>Separated_Pu_Storage</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>Separated_U_Storage</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>EnrichPlant_for_Reactor_Re-use</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>EnrichPlant_for_HEU_Prod</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>HEU_Storage</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
    </institution>
  </region>
</simulation>
