                <leftover_commod>waste</leftover_commod>

            </Separations>
        </config>
    </prototype>
    <prototype>
        <name>FAB</name>
        <config>
            <cyclassFuelFab>

                <eq_model>FBR_MLP_Keff</eq_model>
                <eq_command>/Users/mouginot/work/MODEL/MODEL_TRU/rnr/EQM/weights/TMVARegression_MLP_k_eff.weights.xml,1.034,/Users/mouginot/work/MODEL/MODEL_TRU/rnr/EQM/RNR_MOX_48.1155Wg.nfo</eq_command>
