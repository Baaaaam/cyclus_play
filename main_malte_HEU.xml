<simulation xmlns:xi="http://www.w3.org/2001/XInclude">

    <schematype>flat</schematype>
    <control>
        <duration>500</duration>
        <startmonth>1</startmonth>
        <startyear>2015</startyear>
        <decay>never</decay>
    </control>

    <archetypes>
        <spec> <lib>cycamore</lib> <name>Source</name></spec>
        <spec> <lib>cycamore</lib> <name>DeployInst</name></spec>
        <spec> <lib>cycamore</lib> <name>Sink</name></spec>
        <spec> <lib>cycamore</lib> <name>Reactor</name> </spec>
        <spec> <lib>cycvt</lib> <name>SEnrichment</name> </spec>
    </archetypes>



    <prototype>
        <name>u_nat</name>
        <config>
            <Source>
                <outcommod>unat</outcommod>
                <outrecipe>unat</outrecipe>
                <inventory_size>1e100</inventory_size>
                <throughput>1e100</throughput>
            </Source>
        </config>
    </prototype>

    <prototype>
        <name>enrich</name>
        <config>
            <SEnrichment>
                <feed_commod>unat</feed_commod>
                <feed_recipe>unat</feed_recipe>
                <product_commod>UOX</product_commod>
                <tails_commod>DU</tails_commod>
                <tails_assay>0.003</tails_assay>
                <swu_capacity>1e100</swu_capacity>
                <initial_feed>1e100</initial_feed>
                                
                <enrich_efficiencies>
                    <item>
                        <comp>U236</comp>
                        <eff>0.4504918</eff>
                    </item>
                </enrich_efficiencies>
                
            </SEnrichment>
        </config>
    </prototype>

    <prototype>
        <name>LWR</name>
        <lifetime>960</lifetime>
        <config>
            <Reactor>
                <fuel_inrecipes>  <val>UOX</val></fuel_inrecipes>
                <fuel_outrecipes> <val>UOX_spent</val></fuel_outrecipes>
                <fuel_incommods>  <val>UOX</val></fuel_incommods>
                <fuel_outcommods> <val>waste</val></fuel_outcommods>

                <cycle_time>9</cycle_time>
                <refuel_time>0</refuel_time>
                <assem_size>14784</assem_size>
                <n_assem_core>6</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>900</power_cap>
            </Reactor>
        </config>
    </prototype>
    <prototype>
        <name>sink</name>
        <config>
            <Sink>
                <in_commods><val>waste</val></in_commods>
                <capacity>1e100</capacity>
            </Sink>
        </config>
    </prototype>
    <prototype>
        <name>sink_du</name>
        <config>
            <Sink>
                <in_commods><val>DU</val></in_commods>
                <capacity>1e100</capacity>
            </Sink>
        </config>
    </prototype>

    <prototype>
        <name>deployer_PWR_CHAIN</name>
        <config>
            <DeployInst>
                <prototypes>
                    <val>u_nat</val>
                    <val>enrich</val>
                    <val>sink</val>
                    <val>sink_du</val>
                    <val>LWR</val>
                </prototypes>

                <build_times>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                </build_times>

                <n_build>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                </n_build>
            </DeployInst>
        </config>
    </prototype>


    <agent> <name>deployer</name> <prototype>deployer_PWR_CHAIN</prototype> </agent>



    <recipe>
        <name>unat</name>
        <basis>mass</basis>
        <nuclide> <id>U235</id> <comp>0.0065830</comp> </nuclide>
        
        <nuclide> <id>U236</id> <comp>0.0000683</comp> </nuclide>
        
        <nuclide> <id>U238</id> <comp>0.9933487</comp> </nuclide>
    </recipe>

    <recipe>
        <name>UOX</name>
        <basis>mass</basis>
        <nuclide> <id>U235</id><comp>0.94</comp></nuclide>
        <nuclide><id>U238</id><comp>0.06</comp></nuclide>
    </recipe>

    <recipe>
        <name>UOX_spent_bak</name>
        <basis>mass</basis>
        <nuclide><id>U235</id> <comp>.008</comp> </nuclide>
        <nuclide><id>U238</id> <comp> 0.9245</comp></nuclide>
        <nuclide><id>Pu239</id> <comp>0.0143</comp></nuclide>
        <nuclide><id>Am241</id> <comp>0.0011</comp></nuclide>

        <nuclide> <id>Cs137</id> <comp>0.0521</comp> </nuclide>
    </recipe>

    <recipe>
        <name>UOX_spent</name>
        <basis>mass</basis>
        <nuclide><id>U235</id> <comp>.008</comp> </nuclide>
        <nuclide><id>U238</id> <comp>0.9268</comp></nuclide>
        <nuclide><id>Pu239</id> <comp>0.012</comp></nuclide>
        <nuclide><id>Am241</id> <comp>0.0011</comp></nuclide>

        <nuclide> <id>Cs137</id> <comp>0.0521</comp> </nuclide>
    </recipe>

</simulation>
