<simulation>
    <schematype>flat</schematype>
    
    <control>
        <decay>never</decay>
        <duration>360</duration>
        <explicit_inventory>true</explicit_inventory>
        <startmonth>1</startmonth>
        <startyear>2000</startyear>
        <tolerance_resource>1e-5</tolerance_resource>
    </control>
    
    
    <archetypes>
        <spec>
            <lib>cycamore</lib>  <name>Source</name>
        </spec>
        <spec>
            <lib>cycamore</lib>  <name>DeployInst</name>
        </spec>
        <spec>
            <lib>cycamore</lib>  <name>Sink</name>
        </spec>
        <spec>
            <lib>cycamore</lib>  <name>Enrichment</name>
        </spec>
        <spec>
            <lib>cycamore</lib>  <name>Reactor</name>
        </spec>
        <spec>
            <lib>cycamore</lib>  <name>Storage</name>
        </spec>
        <spec>
            <lib>cycamore</lib>  <name>FuelFab</name>
        </spec>
        <spec>
            <lib>cycamore</lib>  <name>Separations</name>
        </spec>
        <spec>
            <lib>cycamore</lib>  <name>Reactor</name>
        </spec>
    </archetypes>


    <agent>
        <name>deployer1</name>  <prototype>deployer</prototype>
    </agent>

    
    <prototype>
        <config>
            <Storage>
                <in_commods>
                    <val>c_pu</val>
                </in_commods>
                <out_commods>
                    <val>c_pu_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
        <name>str_pu</name>
    </prototype>

    <prototype>
        <config>
            <Storage>
                <in_commods>
                    <val>c_ru</val>
                </in_commods>
                <out_commods>
                    <val>c_ru_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
        <name>str_ru</name>
    </prototype>
    
    <prototype>
        <config>
            <Source>
                <outcommod>c_du_src</outcommod>
                <outrecipe>r_du</outrecipe>
                <throughput>1e100</throughput>
            </Source>
        </config>
        <name>src_depleted</name>
    </prototype>
    
    <prototype>
        <config>
            <Source>
                <outcommod>c_u_src</outcommod>
                <outrecipe>r_natl_u</outrecipe>
            </Source>
        </config>
        <name>src_u_nat</name>
    </prototype>
    
    <prototype>
        <config>
            <Source>
                <outcommod>c_u_src_2</outcommod>
                <outrecipe>r_natl_u</outrecipe>
            </Source>
        </config>
        <name>src_u_nat_2</name>
    </prototype>
    
    <prototype>
        <config>
            <Sink>
                <capacity>1e100</capacity>
                <in_commods>
                    <val>c_waste</val>
                </in_commods>
            </Sink>
        </config>
        <name>waste</name>
    </prototype>
    
    <prototype>
        <config>
            <Storage>
                <in_commods>
                    <val>c_du</val>
                </in_commods>
                <out_commods>
                    <val>c_du_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
        <name>str_du</name>
    </prototype>
    
    <prototype>
        <config>
            <Enrichment>
                <feed_commod>c_u_src</feed_commod>
                <feed_recipe>r_natl_u</feed_recipe>
                <product_commod>c_uox</product_commod>
                <swu_capacity>300000</swu_capacity>
                <tails_assay>0.003</tails_assay>
                <tails_commod>c_du</tails_commod>
                <tails_assay_uncertainty>0.1</tails_assay_uncertainty>
                <product_assay_uncertainty>0.1</product_assay_uncertainty>
                <swu_uncertainty>0.1</swu_uncertainty>
            </Enrichment>
        </config>
        <name>enrichment</name>
    </prototype>
    
    <prototype>
        <config>
            <Storage>
                <in_commods>
                    <val>c_uox</val>
                </in_commods>
                <in_recipe>r_uox</in_recipe>
                <out_commods>
                    <val>c_uox_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
        <name>str_uox</name>
    </prototype>
    
    <prototype>
        <config>
            <DeployInst>
                <build_times>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                </build_times>
                <n_build>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                </n_build>
                <prototypes>
                    <val>waste</val>
                    <val>enrichment</val>
                    <val>src_depleted</val>
                    <val>src_u_nat</val>
                    <val>src_u_nat_2</val>
                    <val>str_du</val>
                    <val>str_ru</val>
                    <val>str_uox</val>
                    <val>str_pu</val>
                </prototypes>
            </DeployInst>
        </config>
        <name>deployer</name>
    </prototype>
    <recipe>
        <basis>mass</basis>
        <name>r_du</name>
        <nuclide>
