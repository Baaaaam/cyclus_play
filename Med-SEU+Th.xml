<simulation xmlns:xi="http://www.w3.org/2001/XInclude">

  <control>
    <duration>1200</duration>
    <startmonth>1</startmonth>
      <startyear>2024</startyear>
    <simhandle>Med-SEU+Th</simhandle>
    <explicit_inventory>false</explicit_inventory>
  </control>
  
  <archetypes>
    <spec><lib>cycamore</lib><name>Enrichment</name></spec>
    <spec><lib>cycamore</lib><name>Reactor</name></spec>
    <spec><lib>cycamore</lib><name>Source</name></spec>
    <spec><lib>cycamore</lib><name>Sink</name></spec>
	<spec><lib>cycamore</lib><name>Storage</name></spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>cycamore</lib><name>DeployInst</name></spec>
    <spec><lib>cycamore</lib><name>FuelFab</name></spec>
    <spec><lib>cycamore</lib><name>Separations</name></spec>
  </archetypes>
  
  <commodity><name>U-ore-comm</name><solution_priority>1.0</solution_priority></commodity>
  <commodity><name>Th-ore-comm</name><solution_priority>1.0</solution_priority></commodity>
  <commodity><name>SEU-comm</name><solution_priority>1.0</solution_priority></commodity>
  <commodity><name>Med-SEU-Th-comm</name><solution_priority>1.0</solution_priority></commodity>
  <commodity><name>Enrich-Tails-comm</name><solution_priority>1.0</solution_priority></commodity>
  <commodity><name>Discharge-Med-SEU-Th-comm</name><solution_priority>1.0</solution_priority></commodity>
  <commodity><name>Cooled-Med-SEU-Th-comm</name><solution_priority>1.0</solution_priority></commodity>
  <commodity><name>Decayed-Med-SEU-Th-comm</name><solution_priority>1.0</solution_priority></commodity>
  
  <facility>
    <name>EnrichPlant</name>
    <config>
      <Enrichment>
        <feed_commod>U-ore-comm</feed_commod>
        <feed_recipe>Nat-U-rec</feed_recipe>
        <max_feed_inventory>3461564</max_feed_inventory>
        <product_commod>SEU-comm</product_commod>
        <tails_commod>Enrich-Tails-comm</tails_commod>
        <tails_assay>0.0025</tails_assay>
      </Enrichment>
    </config>
  </facility>

  <facility>
    <name>U-mine</name>
    <config>
      <Source>
        <outcommod>U-ore-comm</outcommod>
        <!--<outcommod>SEU-comm</outcommod>-->
      </Source>
    </config>
  </facility>
  
  <facility>
    <name>Th-mine</name>
    <config>
      <Source>
        <outcommod>Th-ore-comm</outcommod>
        <!--<outrecipe>Nat-Th-rec</outrecipe>-->
      </Source>
    </config>
  </facility>
  
  <facility>
    <name>Fabrication</name>
    <config>
      <FuelFab>
        <fill_commods><val>Th-ore-comm</val></fill_commods>
        <fill_recipe>Nat-Th-rec</fill_recipe>
        <fill_size>90000</fill_size>
        <fiss_commods><val>SEU-comm</val></fiss_commods>
        <fiss_recipe>SEU-rec</fiss_recipe>
        <fiss_size>2000000</fiss_size>
        <outcommod>Med-SEU-Th-comm</outcommod>
        <spectrum>thermal</spectrum>
      </FuelFab>
    </config>
  </facility>
  
  <!--<facility>
    <name>Fabrication</name>
    <config>
      <Mixer>
        <in_streams>
          <stream><info><mixing_ratio>0.954212963</mixing_ratio><buf_size>10000000</buf_size></info>
            <commodities><item><commodity>SEU-comm</commodity><pref>1.0</pref></item></commodities>
          </stream>
          <stream><info><mixing_ratio>0.045787037</mixing_ratio><buf_size>10000000</buf_size></info>
            <commodities><item><commodity>Th-ore-comm</commodity><pref>1.0</pref></item></commodities>
          </stream>
        </in_streams>
        <out_commod>Med-SEU-Th-comm</out_commod>
      </Mixer>
    </config>
  </facility>-->
  
  <facility>
    <name>PT-HWR</name>
    <config>
      <Reactor>
        <fuel_incommods>
          <val>Med-SEU-Th-comm</val>
        </fuel_incommods>
        <fuel_inrecipes>
          <val>Med-SEU-Th-rec</val>
        </fuel_inrecipes>
        <fuel_outcommods>
          <val>Discharge-Med-SEU-Th-comm</val>
        </fuel_outcommods>
        <fuel_outrecipes>
          <val>SF-Med-SEU-Th-rec</val>
        </fuel_outrecipes>
        <assem_size>2796.748187</assem_size>
        <n_assem_batch>1</n_assem_batch>
        <n_assem_core>31</n_assem_core>
        <cycle_time>1</cycle_time>
        <refuel_time>0</refuel_time>
        <power_cap>578.2227561</power_cap>
      </Reactor>
    </config>
  </facility>
  
  <facility>
    <name>NuclearUnderground</name>
    <config>
      <Sink>
        <in_commods>
          <val>Decayed-Med-SEU-Th-comm</val>
        </in_commods>
        <capacity>119607.4343</capacity>
      </Sink>
    </config>
  </facility>
  <facility>
    <name>WetStorage</name>
    <config>
      <Storage>
        <in_commods>
        <val>Discharge-Med-SEU-Th-comm</val>
        </in_commods>
        <out_commods><val>Cooled-Med-SEU-Th-comm</val></out_commods>
        <residence_time>59</residence_time>
      </Storage> 
    </config>
  </facility>
  <facility>
	<name>DryStorage</name>
	<config>
	  <Storage>
    <in_commods><val>Cooled-Med-SEU-Th-comm</val></in_commods>
		<out_commods><val>Decayed-Med-SEU-Th-comm</val></out_commods>
		<residence_time>299</residence_time>
	  </Storage>
	  </config>	
  </facility>
  <region>
    <name>Nuclandia</name>
    <config>
      <NullRegion/>
    </config>
    <institution>
      <name>MyNucCo</name>
      <config>
        <DeployInst>
          <prototypes>
           <val>PT-HWR</val>
           <val>PT-HWR</val>
           <val>EnrichPlant</val>
           <val>Fabrication</val>
            <val>U-mine</val>
            <val>Th-mine</val>
            <val>WetStorage</val>
            <val>DryStorage</val>
            <val>NuclearUnderground</val>
          </prototypes>
          <build_times>
            <val>13</val>
            <val>373</val>
            <val>2</val>
            <val>2</val>
            <val>1</val>
            <val>1</val>
            <val>12</val>
            <val>50</val>
            <val>434</val>
          </build_times>
          <n_build>
            <val>20</val>
            <val>20</val>
            <val>1</val>
            <val>1</val>
            <val>1</val>
            <val>1</val>
            <val>1</val>
            <val>1</val>
            <val>1</val>
          </n_build>
          <lifetimes>
            <val>360</val>
            <val>360</val>
            <val>2400</val>
            <val>2400</val>
            <val>2400</val>
            <val>2400</val>
            <val>2400</val>
            <val>2400</val>
            <val>2400</val>
          </lifetimes>
        </DeployInst>
      </config>
    </institution>
  </region>
  
<recipe><name>Nat-U-rec</name><basis>mass</basis>	
	<nuclide><id>U235</id><comp>0.007114319</comp></nuclide>
	<nuclide><id>U238</id><comp>0.992885681</comp></nuclide>
</recipe>	
  
  
  <recipe><name>Nat-Th-rec</name><basis>mass</basis>
    <nuclide><id>Th232</id><comp>1.0</comp></nuclide>
  </recipe>
  
  <recipe><name>SEU-rec</name><basis>mass</basis>
    <nuclide><id>U235</id><comp>0.011851908</comp></nuclide>
    <nuclide><id>U238</id><comp>0.988148092</comp></nuclide>
  </recipe>

<recipe><name>Med-SEU-Th-rec</name><basis>mass</basis>	
	<nuclide><id>U235</id><comp>0.0113092713817483</comp></nuclide>
	<nuclide><id>U238</id><comp>0.942906000947316</comp></nuclide>
	<nuclide><id>Th232</id><comp>0.0457847276709357</comp></nuclide>
</recipe>	

<recipe><name>SF-Med-SEU-Th-rec</name><basis>mass</basis>	
	<nuclide><id>TH232</id><comp>0.0443985099894545</comp></nuclide>
	<nuclide><id>U233</id><comp>0.000604514338334776</comp></nuclide>
	<nuclide><id>U235</id><comp>0.00104472916800546</comp></nuclide>
	<nuclide><id>U238</id><comp>0.927804751201269</comp></nuclide>
	<nuclide><id>PU239</id><comp>0.00258754439494993</comp></nuclide>
	<nuclide><id>Cs137</id><comp>0.0235599509079863</comp></nuclide>
</recipe>	



</simulation>
